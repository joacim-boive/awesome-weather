{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "chrome >= 60"
          ]
        },
        "debug": false,
        "loose": true,
        "modules": false,
        "useBuiltIns": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "transform-es2015-modules-commonjs",
          "__coverage__",
          {
            "ignore": "*.+(test|stub).*"
          }
        ]
      ]
    }
  }
}
