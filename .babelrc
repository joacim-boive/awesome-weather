{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "chrome >= 60",
            "firefox >= 47",
            "safari >= 10"
          ]
        },
        "debug": true,
        "loose": true,
        "modules": false,
        "useBuiltIns": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "__coverage__",
          {
            "ignore": "*.+(test|stub).*"
          }
        ]
      ]
    }
  }
}
