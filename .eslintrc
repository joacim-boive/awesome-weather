{
  "parser": "babel-eslint",
  "extends": "./node_modules/eslint-config-google/index.js", //https://google.github.io/styleguide/jsguide.html
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "space-before-function-paren": 0,
    "object-curly-spacing": 0
  }
}
